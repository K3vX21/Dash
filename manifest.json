{
    "name": "Dash",
    "description": "Chrome theme to display graphs",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "theme.js"
    },
    "permissions": ["storage"],
    "options_page": "options.html",
    "chrome_url_overrides" : {
        "newtab": "newtab.html"
    }
}